<div class="search-container search-results-search-bar">
<div class="search-bar-container">
    <input id="search-bar" type="text" autocomplete="off" placeholder="Search by zip code...">
    <div class="drop-down">
        <label for='skill' class="">
            <select name="skills" id="skill-list-options" multipe>
            <option value="Select-skill">Select Knack</option>
            {{#each skills as |skill_option|}}
            {{>skill-search-details skill_option}}
            {{/each}}
            </select>
        </label>
    </div>
</div>
    <span uk-icon="icon: search" class="button search-icon-btn" id="search-submit-btn"></span>
</div>
{{#if providers}}
{{#each providers as |provider|}}
{{>provider-details provider}}
{{/each}}
{{else}}
<div class="provider-container">
    <div>
        <div class="bio-header">
            <h4 id="prov-username">404</h4>
        </div>
        <div class="bio-content">
            <h4>No providers in this area.</h4>
        </div>
    </div>
    <div class="skills-list">
    </div>
</div>
{{/if}}

<script src="/js/search.js"></script>